{
  "name": "gathio",
  "id": "gathio",
  "packaging_format": 1,
  "description": {
    "en": "Self-destructing, shareable, no-registration event pages"
  },
  "version": "1.0.0~ynh1",
  "url": "https://gath.io/",
  "license": "GPL-3.0",
  "maintainer": {
    "name": "Anton StrÃ¶mkvist",
    "email": "anton@stromkvist.com",
    "url": "https://ahst.ro/"
  },
  "requirements": {
    "yunohost": ">= 3.5"
  },
  "multi_instance": true,
  "services": ["nginx"],
  "arguments": {
    "install": [
      {
        "name": "domain",
        "type": "domain",
        "ask": {
          "en": "Choose a domain name for gathio",
          "fr": "Choisissez un nom de domaine pour gathio"
        },
        "example": "example.com"
      },
      {
        "name": "is_public",
        "type": "boolean",
        "ask": {
          "en": "Is it a public application?",
          "fr": "Est-ce une application publique ?"
        },
        "default": true
      },
      {
        "name": "sendgrid_api_key",
        "type": "string",
        "ask": {
          "en": "Enter your Sendgrid API key"
        },
        "example": "TH.iSisaFaKEApIkEYtHISISA.FAKeAPi-keYThiS-ISAFAkEAPIkeYThISIsafAKeAPi",
        "optional": "false"
      }
    ]
  }
}
